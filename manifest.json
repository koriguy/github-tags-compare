{
    "manifest_version": 2,
    "name": "github tags compare",
    "description": "This extension allows the user to compare github tags",
    "version": "1.2",
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage",
        "https://api.github.com/"
    ]
}